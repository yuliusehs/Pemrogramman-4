    public static void main(String[] args) {
        Departemen departemen1 = new Departemen();
        Departemen departemen2 = new Departemen ("A1");
        Departemen departemen3 = new Departemen ("A2, Keuangan");
        
        Division divisi1 = new Division("A3","Tata Usaha","Pa Suharjo");
        Division divisi2 = new Division();
        
        departemen1.tampilkaninfo();
        departemen2.tampilkaninfo();
        departemen3.tampilkaninfo();
        
        divisi1.tampilkaninfo();
        divisi1.HitungGaji();
        divisi2.tampilkaninfo();
        divisi2.HitungGaji();
    }
