package animal;

/**
 *
 * @author Yulius Edward
 */
public class Cat extends Animal implements Pet{
    protected String name;
    
        public Cat(){
            System.out.println("This is Cat");                 
}
        public Cat(String nm){
            System.out.println("This is Kitty" +name);
        }

    @Override
    public String getName(){
        return name;
    }

    @Override
    public void setNama(String nm){
        name = nm;
    }

    @Override
    public void play() {
        System.out.println("Cat wanna play");
    }
    

  
}
